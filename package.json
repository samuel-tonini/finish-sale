{
  "name": "processamento_venda",
  "version": "1.0.0",
  "description": "Aplicação feita para processamento de venda afim de concluir a disciplina de Arquitetura Orientada a Serviço da pós-graduação em Conectividade de Tecnologias da Informação do IFES campus Colatina.",
  "author": "Samuel Tonini",
  "license": "mit",
  "scripts": {
    "start": "npm run build && node dist/main/server.js",
    "debug": "set NODE_ENV=\"development\" && rimraf dist && tsc-watch -p tsconfig-build.json --onFirstSuccess \"nodemon -L --watch ./dist --inspect=0.0.0.0:9229 --nolazy ./dist/main/server.js\"",
    "debug:production": "rimraf dist && tsc-watch -p tsconfig-build.json --onFirstSuccess \"nodemon -L --watch ./dist --inspect=0.0.0.0:9229 --nolazy ./dist/main/server.js\"",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "build:watch": "rimraf dist && tsc -p tsconfig-build.json -w",
    "check": "npm-check -s -u"
  },
  "devDependencies": {
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.20",
    "@types/node-fetch": "^2.5.8",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "eslint": "^7.17.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "tsc-watch": "^4.4.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "cron": "^1.8.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "node-fetch": "^2.6.1",
    "swagger-ui-express": "^4.1.6"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
